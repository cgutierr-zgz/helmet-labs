{
  "sources": [
    {
      "id": "sec_8k_filings",
      "name": "SEC 8-K Filings (Material Events)",
      "url": "https://www.sec.gov/cgi-bin/browse-edgar?action=getcurrent&type=8-K&count=40&output=atom",
      "type": "atom",
      "selector": "entry",
      "title_selector": "title",
      "link_selector": "link[rel='alternate']",
      "date_selector": "updated",
      "interval_seconds": 60,
      "category": "corporate",
      "priority": 1,
      "needs_js": false,
      "user_agent": "TradingBot/1.0 (contact@example.com)",
      "diff_threshold": 0.05,
      "notes": "8-K filings: material events (mergers, acquisitions, earnings, leadership changes). ATOM feed. Requires custom User-Agent. Low threshold (5%) due to high-value, infrequent changes."
    },
    {
      "id": "fed_all_releases",
      "name": "Federal Reserve - All Press Releases",
      "url": "https://www.federalreserve.gov/feeds/press_all.xml",
      "type": "rss",
      "selector": "item",
      "title_selector": "title",
      "link_selector": "link",
      "date_selector": "pubDate",
      "category_selector": "category",
      "interval_seconds": 120,
      "category": "monetary",
      "priority": 1,
      "needs_js": false,
      "notes": "All Fed releases: FOMC statements, rate decisions, banking orders, enforcement. RSS feed works directly."
    },
    {
      "id": "fed_monetary_policy",
      "name": "Federal Reserve - Monetary Policy Only",
      "url": "https://www.federalreserve.gov/feeds/press_monetary.xml",
      "type": "rss",
      "selector": "item",
      "title_selector": "title",
      "link_selector": "link",
      "date_selector": "pubDate",
      "interval_seconds": 300,
      "category": "monetary",
      "priority": 1,
      "needs_js": false,
      "notes": "FOMC statements, rate decisions, discount rate minutes. High-impact market movers."
    },
    {
      "id": "fda_press_announcements",
      "name": "FDA Press Announcements",
      "url": "https://www.fda.gov/about-fda/contact-fda/stay-informed/rss-feeds/press-releases/rss.xml",
      "type": "rss",
      "selector": "item",
      "title_selector": "title",
      "link_selector": "link",
      "date_selector": "pubDate",
      "interval_seconds": 120,
      "category": "regulatory",
      "priority": 1,
      "needs_js": false,
      "notes": "Drug approvals, recalls, safety alerts. Major impact on pharma stocks. RSS feed works directly."
    },
    {
      "id": "treasury_press_releases",
      "name": "Treasury Press Releases",
      "url": "https://home.treasury.gov/news/press-releases",
      "type": "html",
      "selector": ".mm-news-row",
      "title_selector": ".news-title a",
      "link_selector": ".news-title a",
      "date_selector": "time.datetime",
      "interval_seconds": 180,
      "category": "government",
      "priority": 2,
      "needs_js": true,
      "diff_threshold": 0.08,
      "ignore_patterns": [
        "Press Release \\d+-\\d+",
        "Treasury\\.gov ID: \\d+"
      ],
      "notes": "Sanctions, borrowing estimates, policy statements. HTML scraping with JS required. Fixed selectors: .mm-news-row container, .news-title a for links"
    },
    {
      "id": "whitehouse_briefings",
      "name": "White House Briefings & Statements",
      "url": "https://www.whitehouse.gov/briefings-statements/",
      "type": "html",
      "selector": "li.wp-block-post",
      "title_selector": "h2.wp-block-post-title a",
      "link_selector": "h2.wp-block-post-title a",
      "date_selector": ".wp-block-post-date time",
      "interval_seconds": 180,
      "category": "government",
      "priority": 2,
      "needs_js": true,
      "notes": "Presidential statements, executive orders, policy announcements. WordPress site, needs JS rendering. Fixed selectors: li.wp-block-post container"
    },
    {
      "id": "ofac_recent_actions",
      "name": "OFAC Sanctions Recent Actions",
      "url": "https://ofac.treasury.gov/recent-actions",
      "type": "html",
      "selector": ".views-row",
      "title_selector": ".font-sans-lg a",
      "link_selector": ".font-sans-lg a",
      "date_selector": ".font-sans-2xs",
      "interval_seconds": 300,
      "category": "sanctions",
      "priority": 1,
      "needs_js": true,
      "notes": "New sanctions designations. Major impact on affected companies/countries. Fixed selectors after RSS retirement (Jan 31, 2025). Requires JS rendering."
    }
  ],
  "config": {
    "default_interval_seconds": 120,
    "max_concurrent_requests": 3,
    "request_timeout_ms": 15000,
    "rate_limit_per_domain_ms": 2000,
    "max_retries": 3,
    "retry_base_delay_ms": 1000,
    "max_consecutive_errors": 5,
    "user_agent_rotation": true,
    "respect_robots_txt": true,
    "default_diff_threshold": 0.1,
    "_diff_notes": "Semantic diff filters noise (timestamps, session IDs, counters, ads). Per-source: 'diff_threshold' (0.0-1.0, default 0.1=10% change), 'ignore_patterns' (custom regex list)."
  },
  "categories": {
    "corporate": "SEC filings, earnings, M&A",
    "monetary": "Federal Reserve, interest rates",
    "regulatory": "FDA, approval/rejection of drugs",
    "government": "White House, executive actions",
    "sanctions": "OFAC designations, Treasury actions"
  }
}
